<p id="notice"><%= notice %></p>

<h1>Defaultitems</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @defaultitems.each do |defaultitem| %>
      <tr>
        <td><%= link_to 'Show', defaultitem %></td>
        <td><%= link_to 'Edit', edit_defaultitem_path(defaultitem) %></td>
        <td><%= link_to 'Destroy', defaultitem, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Defaultitem', new_defaultitem_path %>
